<%- include('partials/header');%>
<h1>Home</h1>
<p> <%= lorem %></p>
<%post.forEach((item)=>{%>
    <h1><%= item.title%></h1>
    <%if (item.blog.length > 100) { %>
        <p><%= item.blog.substring(0, 99)+" ..." %>
        <a href="post/<%= item.title%>">read more</a>
        </p>
    <% } else { %>
        <p><%= item.blog %></p>
    <% } %>
<% })%>
<%- include('partials/footer');%>